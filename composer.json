{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type":"package",
      "package":{
        "name":"pixelant/pxa-newsletter-subscription",
        "type": "typo3-cms-extension",
        "version":"4.0.0",
        "source":{
          "type":"git",
          "url":"git@bitbucket.org:pixelant/pxa_newsletter_subscription.git",
          "reference":"master"
        }
      },
      "autoload": {
        "psr-4": {
          "Pixelant\\PxaNewsletterSubscription\\": "Classes/"
        }
      },
      "replace": {
        "pxa_newsletter_subscription": "self.version",
        "typo3-ter/pxa-newsletter-subscription": "self.version"
      }
    },
    {
      "type":"package",
      "package":{
        "name":"pixelant/pxa-form-enhancement",
        "type": "typo3-cms-extension",
        "version":"1.0.1",
        "source":{
          "type":"git",
          "url":"git@github.com:pixelant/pxa_form_enhancement.git",
          "reference":"master"
        }
      },
      "autoload": {
        "psr-4": {
          "Pixelant\\PxaFormEnhancement\\": "Classes/"
        }
      },
      "replace": {
        "pxa_form_enhancement": "self.version",
        "typo3-ter/pxa-form-enhancement": "self.version"
      }
    }
  ],
  "require": {
    "typo3/cms": "7.6.9",
    "typo3-ter/theme-t3kit": "1.7.0",
    "typo3-ter/theme-t3kit-bluemountain": "0.4.0",
    "georgringer/news": "4.0.0",
    "clickstorm/go_maps_ext": "2.0.0",
    "typo3-ter/realurl": "2.0.14",
    "typo3-ter/realurl-404-multilingual": "1.0.7",
    "typo3-ter/seo-basics": "0.9.2",
    "typo3-ter/solr": "3.1.1",
    "pixelant/pxa-newsletter-subscription":"4.0.0",
    "pixelant/pxa-form-enhancement":"1.0.1"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "."
    }
  }
}
